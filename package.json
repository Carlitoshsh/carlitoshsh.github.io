{
  "name": "react-test",
  "version": "1.0.0",
  "description": "React testing",
  "main": "index.js",
  "repository": "https://github.com/Carlitoshsh/ReactTest",
  "author": "Carlos Alfonso  Gómez Hernández <caragomezher@unal.edu.co>",
  "license": "MIT",
  "scripts": {
    "start": "parcel ./src/index.html -p 3000 --open",
    "build": "parcel build ./src/index.html",
    "clean": "rm -rf dist",
    "lint": "eslint \"**/*.{ts,tsx}\" --quiet",
    "format": "eslint \"**/*.{ts,tsx}\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.20",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@rmwc/card": "^6.1.3",
    "@rmwc/theme": "^6.1.2",
    "apollo-boost": "^0.4.9",
    "global": "^4.4.0",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.10.4",
    "react": "^16.3.x",
    "react-apollo": "^3.1.5",
    "react-apollo-hooks": "^0.5.0",
    "react-dom": "^16.3.x",
    "react-helmet": "^6.1.0",
    "recharts": "^1.8.5",
    "styled-components": "^5.1.1",
    "styled-reset": "^4.2.1",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.4",
    "@graphql-codegen/typescript": "1.17.4",
    "@graphql-codegen/typescript-operations": "1.17.4",
    "@graphql-codegen/typescript-react-apollo": "1.17.4",
    "@types/jest": "^26.0.3",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/recharts": "^1.8.14",
    "@types/styled-components": "^5.1.1",
    "@types/styled-system": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "eslint": "^7.3.1",
    "eslint-plugin-react": "^7.20.3",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "parcel-bundler": "^1.12.4",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
